<?xml version="1.0" encoding="utf-8"?>
<METRIC PACKAGE_NAME="TSQL.TSQLError">
  <ACTIVE>YES</ACTIVE>
  <ACTION_LIST>
    <ACTION NAME="MATCH IN TREE ELEMENT" >
      <INTERNAL_UID>8033D1FB-9FAB-4fcc-99A8-3ECD667C2889</INTERNAL_UID>
      <ACTIVE>YES</ACTIVE>
    </ACTION>
    <ACTION NAME="PERSISTENCE" >
      <INTERNAL_UID>1AE3873E-AE59-4813-8914-18CF33125148</INTERNAL_UID>
      <ACTIVE>YES</ACTIVE>
    </ACTION>
  </ACTION_LIST>

  <CATEGORY_LIST>
    <CATEGORY NAME="MATCH_ERROR">
      <PROPERTY NAME="VALUE" type="integer" />
      <PROPERTY NAME="TSQL_ERROR" type="pattern">
        <TREE>
          <OR>
            <SIGNATURE>LIST_NV_INSTRUCTIONS</SIGNATURE>
            <SIGNATURE>NODE_49X_VIEW_DEFINITION</SIGNATURE>
          </OR>

          <SUB_TREE_OR>
            <TOKEN NAME="VARIABLE" EQUAL="@@error" />
          </SUB_TREE_OR>

        </TREE>
      </PROPERTY>

      <PROPERTY NAME="TSQL_TRY_CATCH" type="pattern">
        <TREE>
          <OR>
            <SIGNATURE>LIST_NV_INSTRUCTIONS</SIGNATURE>
            <SIGNATURE>NODE_49X_VIEW_DEFINITION</SIGNATURE>
          </OR>

          <SUB_TREE_OR>
            <SIGNATURE NAME="NODE_M90_TRY_CATCH_BLOCK" />
          </SUB_TREE_OR>
        </TREE>
      </PROPERTY>



      <ADD_ACTION NAME="MATCH IN TREE ELEMENT">
        <MATCH>TSQL_ERROR</MATCH>
        <MATCH>TSQL_TRY_CATCH</MATCH>
        <TOKEN>TOKEN_AND_TREE_ELEMENT.TOKEN_VALUE</TOKEN>
        <SIGNATURE>TOKEN_AND_TREE_ELEMENT.SIGNATURE_VALUE</SIGNATURE>
        <RESULT_COUNT>VALUE</RESULT_COUNT>
      </ADD_ACTION>

      <ADD_ACTION NAME="PERSISTENCE">
        <SAVE>VALUE</SAVE>
        <NAME>Use of @@error</NAME>
      </ADD_ACTION>

    </CATEGORY>

  </CATEGORY_LIST>
</METRIC>
