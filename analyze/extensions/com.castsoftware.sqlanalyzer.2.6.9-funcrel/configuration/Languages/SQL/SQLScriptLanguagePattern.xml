<?xml version="1.0" encoding="utf-8"?>
<languagePattern id="SQLScript" UsedByUA="false">
	<!-- identifier -->
	<!--
 identifier><![CDATA[([A-Za-z\x80-\xFF0-9@#$_]+[A-Za-z\x80-\xFF0-9@#$_]*)+([\w@#$_][\w0-9@#$_]*)+]]></identifier
	-->
	<identifier>
		<![CDATA[[_a-zA-Z][_a-zA-Z0-9][A-Za-z\x80-\xFF0-9@#$_]*]]>
	</identifier>
	<!-- comments -->
	<comment>
		<begin>
			<![CDATA[--|REM|REMARK]]>
		</begin>
		<end>
			<![CDATA[\r\n]]>
		</end>
		<nested>false</nested>
	</comment>
	<comment>
		<begin>
			<![CDATA[/\*]]>
		</begin>
		<end>
			<![CDATA[\*/]]>
		</end>
		<multiline>true</multiline>
	</comment>
	<string>
		<begin>
			<![CDATA[']]>
		</begin>
		<end>
			<![CDATA[']]>
		</end>
		<escape>
			<![CDATA[\]]>
		</escape>
	</string>
	<!-- keywords -->
	<keyword>select</keyword>
	<keyword>if</keyword>
	<keyword>for</keyword>
	<keyword>loop</keyword>
	<keyword>then</keyword>
	<keyword>update</keyword>
	<keyword>delete</keyword>
	<keyword>insert</keyword>
	<keyword>merge</keyword>
	<keyword>else</keyword>
	<keyword>elseif</keyword>
	<keyword>case</keyword>
	<keyword>goto</keyword>
	<keyword>when</keyword>
	<keyword>others</keyword>
	<keyword>dbms\_output</keyword>
	<keyword>put\_line</keyword>
	<keyword>begin</keyword>
	<keyword>end</keyword>
	<keyword>count</keyword>
	<keyword>cursor</keyword>
	<keyword>return</keyword>
	<keyword>mod</keyword>
	<keyword>from</keyword>
	<keyword>into</keyword>
	<keyword>in</keyword>
	<keyword>all</keyword>
	<keyword>or</keyword>
	<keyword>and</keyword>
	<keyword>between</keyword>
	<keyword>not</keyword>
	<keyword>is</keyword>
	<keyword>null</keyword>
	<keyword>unique</keyword>
	<keyword>like</keyword>
	<keyword>exists</keyword>
	<keyword>raise</keyword>
	<keyword>exception</keyword>
	<keyword>integer</keyword>
	<keyword>group</keyword>
	<keyword>by</keyword>
	<keyword>=</keyword>
	<keyword>:=</keyword>
	<keyword>\+</keyword>
	<keyword>\-</keyword>
	<keyword>/</keyword>
	<keyword>\(</keyword>
	<keyword>\)</keyword>
	<keyword>\,</keyword>
	<keyword>\;</keyword>
	<keyword>\*</keyword>
	<keyword>\.</keyword>
	<!-- operator -->
	<operator>=</operator>
	<operator>:=</operator>
	<operator>\+</operator>
	<operator>\-</operator>
	<operator>/</operator>
	<operator>\(</operator>
	<operator>\)</operator>
	<operator>\,</operator>
	<operator>\;</operator>
	<operator>\*</operator>
	<operator>\.</operator>
</languagePattern>
