<?xml version="1.0" encoding="utf-8"?>

<languagePattern id="HTML5" UsedByUA="false">

	<comment>
		<begin><![CDATA[[\/][\/]]]></begin>
		<end><![CDATA[[\r\n]]]></end>
		<multiline>false</multiline>
	</comment>	
	<comment>
		<begin><![CDATA[[\#]]]></begin>
		<end><![CDATA[\r\n]]></end>
		<multiline>false</multiline>
	</comment>	
	<comment>
		<begin><![CDATA[\/\*]]></begin>
		<end><![CDATA[\*\/]]></end>
		<nested>false</nested>
		<multiline>true</multiline>
	</comment>	
	<comment>
		<begin><![CDATA[<!--]]></begin>
		<end><![CDATA[-->]]></end>
		<nested>false</nested>
		<multiline>true</multiline>
	</comment>	
	
	<!-- Keywords are used for natural metrics
	Beware: If we enable them, we must enable strings because if not, 
	useLinks to some html files inside strings will disappear if html file names contain a keyword
