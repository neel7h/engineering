  <metadata>
    <id>com.castsoftware.jaxrs</id>
    <version>1.4.0-funcrel</version>
    <title>JAX-RS Annotations</title>
    <authors>CAST Product</authors>
    <owners>CAST</owners>
    <licenseUrl>https://help.castsoftware.com/hc/en-us/articles/212612648-AIP-Extension-License</licenseUrl>
