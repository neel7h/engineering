				<addAttribute name="Validate" value="true" />
				<addAttribute name="Validator" valueSrc="validatorId" />
			</search>
		</tag>

		<!--  Listeners -->
		<tag name="actionListener">
			<attribute name="type" resolveValueAs="java-type"/>
		</tag>
		<tag name="setPropertyActionListener">
			<attribute name="type" resolveValueAs="java-type" ></attribute>
		</tag>
		<tag name="valueChangeListener">
			<attribute name="type" resolveValueAs="java-type"></attribute>
		</tag>


		<!--  Converters -->
		<tag name="converter">
