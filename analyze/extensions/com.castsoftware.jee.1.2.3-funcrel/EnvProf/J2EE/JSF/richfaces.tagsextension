<taglib uri="http://richfaces.org/a4j">

  <!-- commands-->
  <tag name="commandButton">
    <attribute name="action" resolveValueAs="jsf-outcome;jsf-el-method;url" linkType="call"/>
    <attribute name="*"	   resolveValueAs="jsf-el-method" methodType="getter"/>
  </tag>

  <tag name="commandLink">
    <attribute name="action" resolveValueAs="jsf-outcome;jsf-el-method;url" linkType="call"/>
    <attribute name="*"	   resolveValueAs="jsf-el-method" methodType="getter"/>
  </tag>

  <!-- Default processing -->
  <tag name="*">
    <attribute name="*"		resolveValueAs="jsf-el-method"	methodType="getter;method" />
  </tag>
  
</taglib>    

