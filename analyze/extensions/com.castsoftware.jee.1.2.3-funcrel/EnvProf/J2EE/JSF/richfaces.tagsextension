<!-- Rich Faces -->
<taglib uri="http://richfaces.org/rich">
	<tag name="autocomplete">
		<create nameSrc="id" kind="jsf-input-field" />
		<search kind="jsf-input-field">
		<addAttribute name="Required" valueSrc="required"/>
		</search>
		<attribute name="value"	  resolveValueAs="jsf-el-method"  methodType="allAccessors"	caller-type="jsf-input-field" />
		<attribute name="converter" resolveValueAs="jsf-converter"					    caller-type="jsf-input-field" />
		<attribute name="validator" resolveValueAs="jsf-validator;jsf-el-method"	    caller-type="jsf-input-field" >
		<search kind="jsf-input-field">
			<addAttribute name="Validate" value="true"/>
		</search>
		</attribute>
		<attribute name="*"		 resolveValueAs="jsf-el-method"	  methodType="getter;method"   caller-type="jsf-input-field" />
	</tag>
	<tag name="calendar">
		<create nameSrc="id" kind="jsf-input-field" />
		<search kind="jsf-input-field">
		<addAttribute name="Required" valueSrc="required"/>
		</search>
		<attribute name="value"		resolveValueAs="jsf-el-method"	methodType="allAccessors"		caller-type="jsf-input-field"/>
		<attribute name="converter"   resolveValueAs="jsf-converter"					    caller-type="jsf-input-field" />
		<attribute name="validator"   resolveValueAs="jsf-validator;jsf-el-method"	        caller-type="jsf-input-field" >
		<search kind="jsf-input-field">
			<addAttribute name="Validate" value="true"/>
		</search>
		</attribute>
		<attribute name="*"		resolveValueAs="jsf-el-method"	    methodType="getter;method"     caller-type="jsf-input-field"  />
	</tag>
	<tag name="editor">
