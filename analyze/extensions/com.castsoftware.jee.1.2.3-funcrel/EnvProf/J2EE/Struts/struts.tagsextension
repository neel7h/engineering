		<tag name="iterate">
			<declare name-from-attribute="id" scope="NESTED">
				<useTypeOf instanceSrc="type" typeSrc="type" />
			</declare>

			<create nameSrc="id" kind="bean" scope="page">
				<useTypeOf instanceSrc="type" typeSrc="type" />
			</create>

			<search kind="bean" nameSrc="name" scopeSrc="scope">
				<create kind="bean.property" nameSrc="property" />
			</search>

			<declare name-from-attribute="indexId" variable-class="java.lang.Integer" scope="NESTED"/>
			<create nameSrc="indexId" kind="bean" scope="page" type="java.lang.Integer"/>

			<attribute name="type" resolveValueAs="java-type" />


		</tag>

