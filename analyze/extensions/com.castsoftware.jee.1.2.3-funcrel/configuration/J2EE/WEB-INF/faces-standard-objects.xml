<?xml version='1.0' encoding='UTF-8'?>

<!--
-->

<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">

<faces-config>
   <!-- Validator -->
  <validator>
    <validator-id>javax.faces.DoubleRange</validator-id>
    <validator-class>javax.faces.DoubleRangeValidator</validator-class>
  </validator>
  <validator>
    <validator-id>javax.faces.Length</validator-id>
    <validator-class>javax.faces.LengthValidator</validator-class>
  </validator>
  <validator>
    <validator-id>javax.faces.LongRange</validator-id>
    <validator-class>javax.faces.LongRangeValidator</validator-class>
  </validator>

  <!-- converter -->
  <converter>
    <converter-id>javax.faces.BigDecimal</converter-id>
    <converter-class>javax.faces.BigDecimalConverter</converter-class>
  </converter>
  <converter>
    <converter-id>javax.faces.BigInteger</converter-id>
    <converter-class>javax.faces.BigIntegerConverter</converter-class>
  </converter>
  <converter>
    <converter-id>javax.faces.Boolean</converter-id>
    <converter-class>javax.faces.BooleanConverter</converter-class>
  </converter>
  <converter>
    <converter-id>javax.faces.Byte</converter-id>
    <converter-class>javax.faces.ByteConverter</converter-class>
  </converter>
  <converter>
    <converter-id>javax.faces.Character</converter-id>
    <converter-class>javax.faces.CharacterConverter</converter-class>
  </converter>
  <converter>
    <converter-id>javax.faces.Double</converter-id>
    <converter-class>javax.faces.DoubleConverter</converter-class>
  </converter>
  <converter>
    <converter-id>javax.faces.Float</converter-id>
    <converter-class>javax.faces.FloatConverter</converter-class>
  </converter>
  <converter>
    <converter-id>javax.faces.Integer</converter-id>
    <converter-class>javax.faces.IntegerConverter</converter-class>
  </converter>
  <converter>
    <converter-id>javax.faces.Long</converter-id>
    <converter-class>javax.faces.LongConverter</converter-class>
  </converter>
  <converter>
    <converter-id>javax.faces.Short</converter-id>
    <converter-class>javax.faces.ShortConverter</converter-class>
  </converter>
</faces-config>
